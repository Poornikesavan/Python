# Python
#fact_of_fact(4) â†’ 288 # 4! * 3! * 2! * 1! = 288


f= []
def fact(n):
    a = 1
    for i in range(2, n+1):
        a *= i
        if i == n:
            f.append(a)
            return fact(n -1)        
def fact_of_fact(x):
    x = 1
    for i in f:
        x *= i
    return x
n = int(input())
fact(n)
print(fact_of_fact(f))
